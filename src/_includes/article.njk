---
layout: 'base.njk'
---

<section>
    <article class="container flow">
        <h2 class="article__title">{{ title }}</h2>
        <figure>
            <img src="{{ image }}" alt="{{ imageAlt }}">
            <figcaption>
                Caption of image: {{ imageAlt }}
            </figcaption>
        </figure>
        {{ content | safe }}
    </article>
</section>